<form id="frmEditAccomodation" action="">
    <div class="page-info">
        <h1>Editar alojamiento</h1>
        <button type="submit">Guardar</button>
    </div>

    <button type="button" class="toggle-collpase-section-button" data-toggle="collapse" data-target="#propertyDetails">+ Detalles de la propiedad</button>
    <div id="propertyDetails" class="chalet-details collapse">
        <div class="input-group half">
            <label for="slctchaletAccomodationType">Tipo de alojamiento</label>
            <select id="slctchaletAccomodationType" name="slctchaletAccomodationType" value="{{propertyDetails.accomodationType}}">
                <optgroup label="Option group 1">
                    <option value="Option 1">Option 1</option>
                </optgroup> 
                <optgroup label="Option group 2">
                    <option value="Option 2">Option 2</option>
                </optgroup>              
            </select>
        </div>
        <div class="input-group half">
            <label for="txtChaletName">Nombre</label>
            <input id="txtChaletName" name="txtChaletName" type="text" value="{{propertyDetails.name}}">
        </div>
        <div class="input-group half inline-input-group">
            <label for="txtChaletPhoneNumber">Teléfono establecimiento</label>
            <input id="txtChaletPhoneNumber" name="txtChaletPhoneNumber" type="tel" value="{{propertyDetails.phoneNumber}}">
        </div>
        <div class="input-group half">
            <label for="txtChaletEmail">Email</label>
            <input id="txtChaletEmail" name="txtChaletEmail" type="email" value="{{propertyDetails.email}}">
        </div>
        <div class="input-group half">
            <label for="txtChaletWebsite">Sitio web</label>
            <input id="txtChaletWebsite" name="txtChaletWebsite" type="email" value="{{propertyDetails.website}}">
        </div>
        <div class="input-group third">
            <label for="txtChaletMinPpl">Ocupación mínima</label>
            <input id="txtChaletMinPpl" name="txtChaletMinPpl" type="number" value="{{propertyDetails.minOccupancy}}">
        </div>
        <div class="input-group third">
            <label for="txtChaletMaxPpl">Ocupación máxima</label>
            <input id="txtChaletMaxPpl" name="txtChaletMaxPpl" type="number" value="{{propertyDetails.maxOccupancy}}">
        </div>
        <div class="input-group third">
            <label for="txtChaletTourLicense">Licencia turística</label>
            <input id="txtChaletTourLicense" name="txtChaletTourLicense" type="text" value="{{propertyDetails.tourLicense}}">
        </div>
    </div>

